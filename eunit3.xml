	<target name="atl84">
				<!-- Create temporary files for input and output models -->
				<tempfile property="atl.temp.srcfile84" />
				<tempfile property="atl.temp.dstfile84" />


				<!-- Save input model to a file -->
				<epsilon.storeModel model="BibTeX84" target="${atl.temp.srcfile84}" />

				<!-- Load the metamodels and the source model -->
				<atl.loadModel name="BibTeX"   metamodel="MOF" path="metamodels/BibTeX.ecore" />
				<atl.loadModel name="DocBook"  metamodel="MOF" path="metamodels/DocBook.ecore" />
				<atl.loadModel name="BibTeX84m"   metamodel="BibTeX" path="${atl.temp.srcfile84}" />

				<!-- Run ATL and save the model -->
				<atl.launch path="transformation/BibTeX2DocBook.atl">
					<inmodel name="IN"   model="BibTeX84m" />
					<outmodel name="OUT" model="DocBook84" metamodel="DocBook" />
				</atl.launch>
		                
				<atl.saveModel model="DocBook84" path="${atl.temp.dstfile84}" derived="false" />
				
				<!-- Load it back into the EUnit suite -->
				<epsilon.emf.loadModel name="DocBook84"
					modelfile="${atl.temp.dstfile84}" metamodeluri="${DocBook.metamodel.uri}" read="true" store="false" />
				

